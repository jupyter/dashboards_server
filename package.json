    "jupyter-js-services": "^0.10.2",
    "jupyter-js-ui": "^0.11.1",
    "jupyter-js-widgets": "^1.1.2",
    "morgan": "^1.6.1",
    "nconf": "^0.8.2",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "request": "^2.67.0",
    "requirejs": "^2.1.21",
    "rimraf": "^2.5.2",
    "serve-favicon": "^2.3.0",
    "tmp": "0.0.28",
    "url-join": "0.0.1",
    "websocket": "^1.0.22"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "gulp": "^3.9.0",
    "gulp-expect-file": "0.0.7",
    "gulp-less": "^3.0.1",
    "gulp-nodemon": "^2.0.2",
    "gulp-open": "^1.0.0",
    "gulp-plumber": "^1.0.0",
    "jsdom": "^8.1.0",
