    "merge-stream": "^1.0.0",
    "mocha": "^2.3.4",
    "nightmare": "^2.2.0",
    "node-lessify": "^0.1.1",
    "proxyquire": "^1.7.3",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.12"
  }
